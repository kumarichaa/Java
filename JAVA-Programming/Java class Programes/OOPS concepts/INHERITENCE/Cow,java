class Animals
{
void show()
{
System.out.println("eating");
System.out.println("braking");
}
}

class Cow extends to Animals
{
string name;
int age;
void get(string s1,int s2)
{
string name=s1;
age=s2;
}
void show()
{
System.out.println(s1);
System.out.println(s2);
}
public static void main(String args[])
{
Cow c1=new Cow();
c1.show("Black cow",23);
Cow c2=new Cow();
c3.show("Berry cow",33);
Cow c3=new Cow();
c3.show("Babyy cow",63);
}
}
}